<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.snaker.engine.entity.Surrogate" table="wf_surrogate">
        <id name="id" type="java.lang.String">
            <column name="id" length="32"/>
        </id>
        <property name="processName" type="java.lang.String" column="process_Name" length="100"/>
        <property name="operator" type="java.lang.String" column="operator" length="50"/>
        <property name="surrogate" type="java.lang.String" column="surrogate" length="50"/>
        <property name="odate" type="java.lang.String" column="odate" length="50"/>
        <property name="sdate" type="java.lang.String" column="sdate" length="50"/>
        <property name="edate" type="java.lang.String" column="edate" length="50"/>
        <property name="state" type="java.lang.Integer" column="state" length="50"/>
    </class>
</hibernate-mapping>